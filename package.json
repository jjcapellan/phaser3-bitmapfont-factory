{
  "name": "phaser3-text-to-bitmapfont",
  "version": "1.0.0",
  "description": "Loads any font as bitmapFont in Phaser3",
  "main": "dist/bmff.js",
  "umd:main": "dist/bmff.umd.js",
  "module": "dist/bmff.ems.js",
  "source": "src/bmff.ts",
  "types": "dist/bmff.d.ts",
  "scripts": {
    "build:dev": "microbundle -f umd --name BMFFactory -o devbuild --no-generateTypes",
    "build:dist": "microbundle -f cjs,umd,es --name BMFFactory --sourcemap false",
    "dev": "microbundle watch -f umd --name BMFFactory"
  },
  "keywords": [
    "Phaser3",
    "bitmapFont",
    "font",
    "text",
    "converter",
    "loader"
  ],
  "author": "Juan Jose Capellan",
  "license": "MIT",
  "devDependencies": {
    "microbundle": "^0.14.2",
    "phaser": "^3.55.2"
  }
}
